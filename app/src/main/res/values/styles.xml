<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="test1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dp</item>
    </style>

    <style name="imageViewCorner" parent="">
        <item name="cornerSizeTopRight">8dp</item>
        <item name="cornerSizeTopLeft">8dp</item>
        <item name="cornerSizeBottomLeft">8dp</item>
        <item name="cornerSizeBottomRight">8dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="imageViewRound" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="publicSwitch" parent="Theme.AppCompat.Light">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">@color/primary</item>

        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">#444444
        </item>

        <!-- inactive track color (30% transparency) -->
        <item name="android:colorForeground">#777777
        </item>
    </style>

    <style name="calenderViewCustom" parent="Theme.AppCompat">

        <item name="colorAccent">@color/primary</item>
        <item name="android:textColorPrimary">@color/black</item>
    </style>

    <style name="editTextTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorControlNormal">@color/primary</item>
        <item name="colorControlActivated">@color/primary</item>
        <item name="colorControlHighlight">@color/primary</item>
    </style>
</resources>